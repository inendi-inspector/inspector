# Rule created to parse syslog
revision = 3

pcre =(\w+\s+\d+\s+\d+:\d+:\d+) (\w+) ([a-zA-Z/]+)\S*: (.*)

axes {
   time 24h default Time
   enum default default Machine
   enum default default Service
   string ascii-val default Log
}

# Test Samples
#Feb  9 18:17:01 xasus /USR/SBIN/CRON[5611]: (root) CMD (   cd / && run-parts --report /etc/cron.hourly)
